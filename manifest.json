{
    "manifest_version":2,
    "name": "Browser Extension",
    "version": "1.0",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action":{
        "default_icon": "snake-game-512.png",
        "default_popup": "Snake/snake.html",
        "default_title": "Sarpe..."

    }
}


